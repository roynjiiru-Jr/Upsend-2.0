{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "upsend",
  "compatibility_date": "2025-12-02",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "upsend-production",
      "database_id": "will-be-filled-on-production"
    }
  ],
  "r2_buckets": [
    {
      "binding": "IMAGES",
      "bucket_name": "upsend-images"
    }
  ]
}